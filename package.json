    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/preset-env": "^7.14.0",
    "@johh/three-effectcomposer": "github:johh/three-effectcomposer",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-minify": "^0.4.3",
    "body-parser": "^1.20.1",
    "chokidar-cli": "^1.2.3",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^6.7.2",
    "css-properties-values": "^1.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^4.18.2",
    "express": "^4.18.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "less": "^3.9.0",
